<h2>Your Posts</h2>

<button type="button" class="btn"><a href="/dashboard/new" class="link">+ New Post</a></button>

<section>
    {{#if posts.length}}
        {{#each posts as |post|}}
            {{> dashboard-postinfo post}}
        {{/each}}
    {{/if}}
</section>

<script src="/js/add-post.js"></script>
